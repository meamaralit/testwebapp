{
  "routes": [
    {
      "route": "/**/*.html",
      "headers": {
        "Cache-Control": "public, max-age=300, must-revalidate"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/images/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/js/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/css/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/api/*",
      "headers": {
        "Cache-Control": "no-store, no-cache, must-revalidate"
      }
    }
  ]
}
